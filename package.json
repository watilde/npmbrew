{
  "name": "npmbrew",
  "version": "1.4.2",
  "description": "a npm version manager",
  "main": "index.js",
  "bin": {
    "npmbrew": "./bin/npmbrew.js"
  },
  "scripts": {
    "lint": "eslint ./lib/*.js && eslint ./lib/modules/*.js && eslint ./test/*.js",
    "tap": "NODE_ENV=development tap test/*.js",
    "test": "npm run lint && npm run tap"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/watilde/npmbrew.git"
  },
  "keywords": [
    "npm"
  ],
  "author": "Daijiro Wachi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/watilde/npmbrew/issues"
  },
  "homepage": "https://github.com/watilde/npmbrew",
  "dependencies": {
    "chalk": "^1.0.0",
    "mkdirp": "^0.5.0",
    "rimraf": "^2.3.2",
    "string-length": "^1.0.0",
    "text-table": "^0.2.0",
    "update-notifier": "^0.3.1"
  },
  "devDependencies": {
    "eslint": "^0.18.0",
    "tap": "^0.7.1"
  }
}
